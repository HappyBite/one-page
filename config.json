  "settings_schema": {
    "theme": {
      "navbar": {
        "title": "Topparea",
        "ui:css_selector": ".navbar-custom",
        "properties": {
          "bg_color": {
            "type": "string",
            "title": "Bakgrundsfärg",
            "format": "color",
            "ui:css_selector": ".navbar-custom",
            "ui:css_property": "background-color"
          },
          "padding_top": {
            "type": "integer",
            "title": "Padding top",
            "minimum": 0,
            "maximum": 30
          },
          "padding_bottom": {
            "type": "integer",
            "title": "Padding bottom",
            "minimum": 0,
            "maximum": 30
          },
          "border_bottom_width": {
            "type": "integer",
            "title": "border bottom width",
            "minimum": 0,
            "maximum": 10
          },
          "border_bottom_color": {
            "type": "string",
            "title": "Border bottom color",
            "format": "color",
            "ui:css_selector": ".navbar-custom",
            "ui:css_property": "border-color"
          }
        }
      },
      "logo": {
        "title": "Logotyp",
        "ui:css_selector": ".navbar-brand",
        "properties": {
          "text_color": {
            "type": "string",
            "title": "Textfärg",
            "format": "color",
            "ui:css_selector": ".navbar-brand",
            "ui:css_property": "color"
          },
          "text_hover_color": {
            "type": "string",
            "title": "Textfärg Hover",
            "format": "color"
          },
          "text_focus_color": {
            "type": "string",
            "title": "Textfärg focus",
            "format": "color"
          },
          "image": {
            "type": "string",
            "title": "Logo",
            "format": "file"
          },
          "padding_top": {
            "type": "integer",
            "title": "Logo padding top",
            "minimum": 0,
            "maximum": 30
          },
          "width": {
            "type": "integer",
            "title": "Logo width in pixels",
            "minimum": 0,
            "maximum": 500
          },
          "show": {
            "type": "boolean",
            "title": "Visa logobild?"
          }
        }
      },
      "navigation": {
        "title": "Navigation",
        "ui:css_selector": ".navbar-nav, .navbar-custom .navbar-toggle",
        "properties": {
          "text_color": {
            "type": "string",
            "title": "Textfärg",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a",
            "ui:css_property": "color"
          },
          "text_hover_color": {
            "type": "string",
            "title": "Textfärg hover",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a:hover",
            "ui:css_property": "color"
          },
          "text_active_color": {
            "type": "string",
            "title": "Textfärg active",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a:active",
            "ui:css_property": "color"
          },
          "text_focus_color": {
            "type": "string",
            "title": "Textfärg focus",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a:focus",
            "ui:css_property": "color"
          },
          "bg_text_color": {
            "type": "string",
            "title": "Bakgrund på text",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a",
            "ui:css_property": "background-color"
          },
          "bg_text_hover_color": {
            "type": "string",
            "title": "Bakgrund på text - hover",
            "format": "color"
          },
          "bg_text_active_color": {
            "type": "string",
            "title": "Bakgrund på text - active",
            "format": "color"
          },
          "bg_text_focus_color": {
            "type": "string",
            "title": "Bakgrund på text - focus",
            "format": "color"
          },
          "mobile_menu_button_bg_color": {
            "type": "string",
            "title": "Hamburgermeny-knapp - bakgrund",
            "format": "color"
          },
          "mobile_menu_button_hover_bg_color": {
            "type": "string",
            "title": "Hamburgermeny-knapp hover - bakgrund",
            "format": "color"
          },
          "mobile_menu_button_focus_bg_color": {
            "type": "string",
            "title": "Hamburgermeny-knapp focus - bakgrund",
            "format": "color"
          }
        }
      },
      "colors": {
        "title": "Färger",
        "ui:css_selector": "body",
        "properties": {
          "body_bg_color": {
            "type": "string",
            "title": "Sidbakgrund",
            "format": "color",
            "ui:css_selector": "body",
            "ui:css_property": "background-color"
          },
          "heading_text_color": {
            "type": "string",
            "title": "Rubrikfärg",
            "format": "color",
            "ui:css_selector": "h1, h2, h3, h4, h5, h6",
            "ui:css_property": "color"
          },
          "text_color": {
            "type": "string",
            "title": "Textfärg",
            "format": "color",
            "ui:css_selector": "p",
            "ui:css_property": "color"
          },
          "link_color": {
            "type": "string",
            "title": "Länk textfärg",
            "format": "color"
          },
          "link_hover_color": {
            "type": "string",
            "title": "Länk textfärg hover",
            "format": "color"
          },
          "primary_bg_color": {
            "type": "string",
            "title": "Primary background color",
            "format": "color",
            "ui:css_selector": ".btn-primary",
            "ui:css_property": "background-color"
          },
          "primary_text_color": {
            "type": "string",
            "title": "Primary text color",
            "format": "color",
            "ui:css_selector": ".text-primary",
            "ui:css_property": "color"
          },
          "selection_text_color": {
            "type": "string",
            "title": "Text selection color",
            "format": "color"
          },
          "selection_bg_color": {
            "type": "string",
            "title": "Text selection bg color",
            "format": "color"
          }
        }
      },
      "typography": {
        "title": "Typography",
        "ui:css_selector": "body",
        "properties": {
          "type_base_size": {
            "type": "integer",
            "minimum": 10,
            "maximum": 18,
            "title": "Basstorlek"
          }
        }
      },
      "header": {
        "title": "Header",
        "ui:css_selector": "header",
        "properties": {
          "lead_in_text_color": {
            "type": "string",
            "title": "Lead in text color",
            "format": "color",
            "ui:css_selector": "header .intro-text .intro-lead-in",
            "ui:css_property": "color"
          },
          "heading_text_color": {
            "type": "string",
            "title": "Heading text color",
            "format": "color",
            "ui:css_selector": "header .intro-text .intro-heading",
            "ui:css_property": "color"
          },
          "button_bg_color": {
            "type": "string",
            "title": "Button bg color",
            "format": "color"
          }
        }
      }
    },
    "page": {
      "data": {
        "title": "Titel och beskrivning",
        "properties": {
          "title": {
            "type": "string",
            "title": "Sidtitel"
          },
          "description": {
            "type": "text",
            "title": "Beskrivning"
          }
        }
      }
    },
    "blocks": {
