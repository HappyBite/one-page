{
  "name": "agency",
  "version": "0.0.0",
  "block_templates": [
    {
      "title": "Navigation",
      "id": "navigation",
      "icon": "navicon"
    },
    {
      "title": "Header",
      "id": "header",
      "icon": "image"
    },
    {
      "title": "Services",
      "id": "services",
      "icon": "file-text-o"
    },
    {
      "title": "Text",
      "id": "teasers",
      "icon": "file-text-o"
    },
    {
      "title": "Footer",
      "id": "footer",
      "icon": "address-card-o"
    },
    {
      "title": "Test",
      "id": "test",
      "icon": "file"
    }
  ],
  "settings_schema": {
    "theme": {
      "typography": {
        "title": "Typography",
        "properties": {
          "type_base_size": {
            "type": "string",
            "title": "Basstorlek",
            "default": "16px"
          }
        }
      },
      "colors": {
        "title": "Färger",
        "properties": {
          "body_bg_color": {
            "type": "string",
            "title": "Sidbakgrund",
            "format": "color",
            "ui:css_selector": "body",
            "ui:css_property": "background-color"
          },
          "heading_color": {
            "type": "string",
            "title": "Rubrikfärg",
            "format": "color",
            "ui:css_selector": "h1, h2, h3, h4, h5, h6",
            "ui:css_property": "color"
          },
          "text_color": {
            "type": "string",
            "title": "Textfärg",
            "format": "color",
            "ui:css_selector": "p",
            "ui:css_property": "color"
          },
          "primary_bg_color": {
            "type": "string",
            "title": "Primary background color",
            "format": "color",
            "ui:css_selector": ".btn-primary",
            "ui:css_property": "background-color"
          },
          "primary_text_color": {
            "type": "string",
            "title": "Primary text color",
            "format": "color",
            "ui:css_selector": ".text-primary, .navbar-brand",
            "ui:css_property": "color"
          }
        }
      },
      "page_settings": {
        "title": "Sidinställningar",
        "properties": {
          "page_title": {
            "type": "string",
            "title": "Sidtitel"
          }
        }
      }
    },
    "blocks": {
      "navigation": {
        "title": "Navigation",
        "properties": {
          "logo": {
            "type": "string",
            "title": "Logo" 
          },
          "logo_text": {
            "type": "string",
            "title": "Logotext" 
          },
          "navigation": {
            "type": "array",
            "title": "Navigation",
            "items": {
              "type": "object",
              "title": "Länk",
              "display_field": "title",
              "properties": {
                "title": {
                  "type": "string",
                  "title": "Title" 
                },
                "link": {
                  "type": "string",
                  "title": "Länk" 
                }
              }
            }
          }
        }
      },
      "header": {
        "title": "Header",
        "properties": {
          "lead_in": {
            "type": "string",
            "title": "Lead in"
          },
          "heading": {
            "type": "string",
            "title": "Rubrik" 
          },
          "link": {
            "type": "string",
            "title": "Länk" 
          },
          "link_text": {
            "type": "string",
            "title": "Länktext" 
          },
          "image": {
            "type": "filepicker",
            "title": "Bild"
          }
        }
      },
      "services": {
        "title": "Services",
        "properties": {
          "heading": {
            "type": "string",
            "title": "Rubrik" 
          },
          "sub_heading": {
            "type": "string",
            "title": "Underrubrik" 
          },
          "items": {
            "type": "array",
            "title": "Services",
            "items": {
              "type": "object",
              "title": "Service",
              "display_field": "heading",
              "properties": {
                "icon": {
                  "type": "string",
                  "title": "Icon" 
                },
                "heading": {
                  "type": "string",
                  "title": "Rubrik" 
                },
                "text": {
                  "type": "text",
                  "title": "Text" 
                }
              }
            }
          }
        }
      },
      "hero": {
        "title": "Hero",
        "properties": {
          "heading": {
            "type": "string",
            "title": "Rubrik" 
          },
          "text": {
            "type": "text",
            "title": "Text",
            "field": {
              "height": "155"
            }
          },
          "image": {
            "type": "filepicker",
            "title": "Bild"
          },
          "link": {
            "type": "string",
            "title": "uri" 
          },
          "link_text": {
            "type": "string",
            "title": "Länktext" 
          }
        }
      }
    },
    "sidebars": {
    }
  },
