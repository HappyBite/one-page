{
  "name": "agency",
  "version": "0.0.0",
  "block_templates": [
    {
      "title": "Navigation",
      "id": "navigation",
      "icon": "navicon"
    },
    {
      "title": "Header",
      "id": "header",
      "icon": "image"
    },
    {
      "title": "Services",
      "id": "services",
      "icon": "file-text-o"
    },
    {
      "title": "Text",
      "id": "teasers",
      "icon": "file-text-o"
    },
    {
      "title": "Footer",
      "id": "footer",
      "icon": "address-card-o"
    },
    {
      "title": "Test",
      "id": "test",
      "icon": "file"
    }
  ],
  "settings_schema": {
    "theme": {
      "navbar": {
        "title": "Topparea",
        "ui:css_selector": ".navbar-custom",
        "properties": {
          "bg_color": {
            "type": "string",
            "title": "Bakgrundsfärg",
            "format": "color"
          },
          "padding_top": {
            "type": "integer",
            "title": "Padding top",
            "minimum": 0,
            "maximum": 30
          },
          "padding_bottom": {
            "type": "integer",
            "title": "Padding bottom",
            "minimum": 0,
            "maximum": 30
          },
          "border_bottom_width": {
            "type": "integer",
            "title": "border bottom width",
            "minimum": 0,
            "maximum": 10
          },
          "border_bottom_color": {
            "type": "string",
            "title": "Border bottom color",
            "format": "color"
          }
        }
      },
      "logo": {
        "title": "Logotyp",
        "ui:css_selector": ".navbar-brand",
        "properties": {
          "text_color": {
            "type": "string",
            "title": "Textfärg",
            "format": "color",
            "ui:css_selector": ".navbar-brand",
            "ui:css_property": "color"
          },
          "text_hover_color": {
            "type": "string",
            "title": "Textfärg Hover",
            "format": "color"
          },
          "text_focus_color": {
            "type": "string",
            "title": "Textfärg focus",
            "format": "color"
          }
        }
      },
      "navigation": {
        "title": "Navigation",
        "ui:css_selector": ".navbar-nav, .navbar-custom .navbar-toggle",
        "properties": {
          "text_color": {
            "type": "string",
            "title": "Textfärg",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a",
            "ui:css_property": "color"
          },
          "text_hover_color": {
            "type": "string",
            "title": "Textfärg hover",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a:hover",
            "ui:css_property": "color"
          },
          "text_active_color": {
            "type": "string",
            "title": "Textfärg active",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a:active",
            "ui:css_property": "color"
          },
          "text_focus_color": {
            "type": "string",
            "title": "Textfärg focus",
            "format": "color",
            "ui:css_selector": ".navbar-custom .nav li a:focus",
            "ui:css_property": "color"
          },
          "bg_text_color": {
            "type": "string",
            "title": "Bakgrund på text",
            "format": "color"
          },
          "bg_text_hover_color": {
            "type": "string",
            "title": "Bakgrund på text - hover",
            "format": "color"
          },
          "bg_text_active_color": {
            "type": "string",
            "title": "Bakgrund på text - active",
            "format": "color"
          },
          "bg_text_focus_color": {
            "type": "string",
            "title": "Bakgrund på text - focus",
            "format": "color"
          },
          "mobile_menu_button_bg_color": {
            "type": "string",
            "title": "Hamburgermeny-knapp - bakgrund",
            "format": "color"
          },
          "mobile_menu_button_hover_bg_color": {
            "type": "string",
            "title": "Hamburgermeny-knapp hover - bakgrund",
            "format": "color"
          },
          "mobile_menu_button_focus_bg_color": {
            "type": "string",
            "title": "Hamburgermeny-knapp focus - bakgrund",
            "format": "color"
          }
        }
      },
      "colors": {
        "title": "Färger",
        "ui:css_selector": "body",
        "properties": {
          "body_bg_color": {
            "type": "string",
            "title": "Sidbakgrund",
            "format": "color",
            "ui:css_selector": "body",
            "ui:css_property": "background-color"
          },
          "heading_text_color": {
            "type": "string",
            "title": "Rubrikfärg",
            "format": "color",
            "ui:css_selector": "h1, h2, h3, h4, h5, h6",
            "ui:css_property": "color"
          },
          "text_color": {
            "type": "string",
            "title": "Textfärg",
            "format": "color",
            "ui:css_selector": "p",
            "ui:css_property": "color"
          },
          "link_color": {
            "type": "string",
            "title": "Länk textfärg",
            "format": "color"
          },
          "link_hover_color": {
            "type": "string",
            "title": "Länk textfärg hover",
            "format": "color"
          },
          "text_muted_color": {
            "type": "string",
            "title": "Textfärg muted",
            "format": "color",
            "ui:css_selector": ".text_muted",
            "ui:css_property": "color"
          },
          "primary_bg_color": {
            "type": "string",
            "title": "Primary background color",
            "format": "color",
            "ui:css_selector": ".btn-primary",
            "ui:css_property": "background-color"
          },
          "primary_text_color": {
            "type": "string",
            "title": "Primary text color",
            "format": "color",
            "ui:css_selector": ".text-primary",
            "ui:css_property": "color"
          },
          "selection_text_color": {
            "type": "string",
            "title": "Text selection color",
            "format": "color"
          },
          "selection_bg_color": {
            "type": "string",
            "title": "Text selection bg color",
            "format": "color"
          }
        }
      },
      "typography": {
        "title": "Typography",
        "ui:css_selector": "body",
        "properties": {
          "type_base_size": {
            "type": "integer",
            "minimum": 10,
            "maximum": 18,
            "title": "Basstorlek"
          }
        }
      },
      "header": {
        "title": "Header",
        "ui:css_selector": "header",
        "properties": {
          "lead_in_text_color": {
            "type": "string",
            "title": "Lead in text color",
            "format": "color",
            "ui:css_selector": "header .intro-text .intro-lead-in",
            "ui:css_property": "color"
          },
          "heading_text_color": {
            "type": "string",
            "title": "Heading text color",
            "format": "color",
            "ui:css_selector": "header .intro-text .intro-heading",
            "ui:css_property": "color"
          },
          "button_bg_color": {
            "type": "string",
            "title": "Button bg color",
            "format": "color"
          }
        }
      }
    },
    "page": {
      "data": {
        "title": "Titel och beskrivning",
        "properties": {
          "title": {
            "type": "string",
            "title": "Sidtitel"
          },
          "description": {
            "type": "text",
            "title": "Beskrivning"
          }
        }
      }
    },
    "blocks": {
      "navigation": {
        "title": "Navigation",
        "properties": {
          "logo": {
            "type": "string",
            "title": "Logo" 
          },
          "logo_show": {
            "type": "boolean",
            "title": "Visa logobild?"
          },
          "logo_text": {
            "type": "string",
            "title": "Logotext" 
          },
          "logo_margin_top": {
            "type": "integer",
            "title": "Padding top",
            "minimum": 0,
            "maximum": 30
          },
          "navigation": {
            "type": "array",
            "title": "Navigation",
            "items": {
              "type": "object",
              "title": "Länk",
              "display_field": "title",
              "properties": {
                "title": {
                  "type": "string",
                  "title": "Title",
                  "default": "New link"
                },
                "link": {
                  "type": "string",
                  "title": "Länk",
                  "default": "#"
                }
              }
            }
          }
        }
      },
      "header": {
        "title": "Header",
        "properties": {
          "lead_in": {
            "type": "string",
            "title": "Lead in"
          },
          "heading": {
            "type": "string",
            "title": "Rubrik" 
          },
          "link": {
            "type": "string",
            "title": "Länk" 
          },
          "link_text": {
            "type": "string",
            "title": "Länktext" 
          },
          "image": {
            "type": "filepicker",
            "title": "Bild"
          }
        }
      },
      "services": {
        "title": "Services",
        "properties": {
          "heading": {
            "type": "string",
            "title": "Rubrik" 
          },
          "sub_heading": {
            "type": "string",
            "title": "Underrubrik" 
          },
          "items": {
            "type": "array",
            "title": "Services",
            "items": {
              "type": "object",
              "title": "Service",
              "display_field": "heading",
              "properties": {
                "icon": {
                  "type": "string",
                  "title": "Icon" 
                },
                "heading": {
                  "type": "string",
                  "title": "Rubrik" 
                },
                "text": {
                  "type": "text",
                  "title": "Text" 
                }
              }
            }
          }
        }
      },
      "hero": {
        "title": "Hero",
        "properties": {
          "heading": {
            "type": "string",
            "title": "Rubrik" 
          },
          "text": {
            "type": "text",
            "title": "Text",
            "field": {
              "height": "155"
            }
          },
          "image": {
            "type": "filepicker",
            "title": "Bild"
          },
          "link": {
            "type": "string",
            "title": "uri" 
          },
          "link_text": {
            "type": "string",
            "title": "Länktext" 
          }
        }
      },
      "footer": {
        "title": "Sidfot",
        "properties": {
          "copyright_text": {
            "type": "string",
            "title": "Copyright text" 
          },
          "twitter_link": {
            "type": "string",
            "title": "Twitter",
            "format": "uri"
          },
          "facebook_link": {
            "type": "string",
            "title": "Facebook",
            "format": "uri"
          },
          "instagram_link": {
            "type": "string",
            "title": "Instagram",
            "format": "uri"
          },
          "linkedin_link": {
            "type": "string",
            "title": "Linked In",
            "format": "uri"
          },
          "quick_links": {
            "type": "array",
            "title": "Snabblänkar",
            "items": {
              "type": "object",
              "title": "Länk",
              "display_field": "title",
              "properties": {
                "title": {
                  "type": "string",
                  "title": "Titel" 
                },
                "link": {
                  "type": "string",
                  "title": "Länk",
                  "format": "uri"
                }
              }
            }
          }
        }
      }
    },
    "sidebars": {
    }
  },
