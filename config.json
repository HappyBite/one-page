{
  "name": "agency",
  "version": "0.0.0",
  "block_templates": [
    {
      "title": "Navigation",
      "id": "navigation",
      "icon": "navicon"
    },
    {
      "title": "Header",
      "id": "header",
      "icon": "image"
    },
    {
      "title": "Services",
      "id": "services",
      "icon": "file-text-o"
    },
    {
      "title": "Team",
      "id": "team",
      "icon": "group"
    },
    {
      "title": "Contact",
      "id": "contact",
      "icon": "address-card-o"
    },
    {
      "title": "Footer",
      "id": "footer",
      "icon": "address-card-o"
    }
