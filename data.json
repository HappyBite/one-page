{"theme":{},"blocks":{"services-1492042267762":{"template_id":"services","content":"<!-- Services Section -->\n<section id=\"services\">\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-lg-12 text-center\">\n          <h2 class=\"section-heading\" data-field-string=\"heading\">{{heading}}</h2>\n          <h3 class=\"section-subheading text-muted\" data-field-string=\"sub_heading\">{{sub_heading}}</h3>\n      </div>\n    </div>\n    <div class=\"row text-center\">\n      {% for item in items %}\n        <div class=\"col-md-4\">\n          <span class=\"fa-stack fa-4x\">\n            <i class=\"fa fa-circle fa-stack-2x text-primary\"></i>\n            <i class=\"fa fa-{{ item.icon }} fa-stack-1x fa-inverse\"></i>\n          </span>\n          <h4\n            class=\"service-heading\"\n            data-field-string=\"services[{{loop.index0}}].heading\"\n          >{{ item.heading }}</h4>\n          <p\n            class=\"text-muted\"\n            data-field-string=\"services[{{loop.index0}}].text\"\n          >{{ item.text }}</p>\n        </div>\n      {% endfor %}\n    </div>\n  </div>\n</section>\n"},"header-1492042250177":{"template_id":"header","content":"<!-- Header -->\n<header style=\"background-image: url({{image.url}})\">\n  <div class=\"container\">\n    <div class=\"intro-text\">\n      <div class=\"intro-lead-in\" data-field-string=\"lead_in\">{{lead_in}}</div>\n      <div class=\"intro-heading\" data-field-string=\"heading\">{{heading}}</div>\n      <a href=\"{{link}}\" class=\"page-scroll btn btn-xl\" data-field-string=\"link_text\" data-field-uri=\"link\">{{link_text}}</a>\n    </div>\n  </div>\n</header>"}}}