{"blocks":{"navigation-1492323242635":{"template_id":"navigation","content":"<!-- Navigation -->\n<nav id=\"mainNav\" class=\"navbar navbar-default navbar-custom navbar-fixed-top\">\n  <div class=\"container\">\n    <!-- Brand and toggle get grouped for better mobile display -->\n    <div class=\"navbar-header page-scroll\">\n      <button type=\"button\" class=\"navbar-toggle\" data-toggle=\"collapse\" data-target=\"#bs-example-navbar-collapse-1\">\n        <span class=\"sr-only\">Toggle navigation</span> Menu <i class=\"fa fa-bars\"></i>\n      </button>\n      <a\n        class=\"navbar-brand page-scroll\"\n        href=\"#page-top\"\n        data-field-string=\"logo_text\"\n        data-toolbar-buttons=\"['bold', 'italic', 'underline', 'anchor']\"\n      >{{ logo_text }}</a>\n    </div>\n    <!-- Collect the nav links, forms, and other content for toggling -->\n    <div class=\"collapse navbar-collapse\" id=\"bs-example-navbar-collapse-1\">\n      <div class=\"nav navbar-nav navbar-right\">\n        <li class=\"hidden\">\n          <a href=\"#page-top\"></a>\n        </li>\n        {% for item in navigation %}\n          <li>\n            <a\n              class=\"page-scroll\"\n              href=\"{{ item.link }}\"\n              data-field-string=\"navigation[{{loop.index0}}].title\"\n              data-field-uri=\"navigation[{{loop.index0}}].link\"\n              data-toolbar-buttons=\"['anchor']\"\n            >{{ item.title }}</a>\n          </li>\n        {% endfor %}\n      </div>\n    </div>\n    <!-- /.navbar-collapse -->\n  </div>\n  <!-- /.container-fluid -->\n</nav>\n"},"services-1492325663372":{"template_id":"services","content":"<!-- Services Section -->\n<section id=\"services\">\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-lg-12 text-center\">\n          <h2\n            class=\"section-heading\"\n            data-field-string=\"heading\"\n            data-toolbar-buttons=\"['bold', 'italic', 'underline', 'anchor']\"\n          >{{heading}}</h2>\n          <h3\n            class=\"section-subheading text-muted\"\n            data-field-string=\"sub_heading\"\n            data-toolbar-buttons=\"['bold', 'italic', 'underline', 'anchor']\"\n          >{{sub_heading}}</h3>\n      </div>\n    </div>\n    <div class=\"row text-center\">\n      {% for item in items %}\n        <div class=\"col-md-4\">\n          <span class=\"fa-stack fa-4x\">\n            <i class=\"fa fa-circle fa-stack-2x text-primary\"></i>\n            <i class=\"fa fa-{{ item.icon }} fa-stack-1x fa-inverse\"></i>\n          </span>\n          <h4\n            class=\"service-heading\"\n            data-field-string=\"items[{{loop.index0}}].heading\"\n            data-toolbar-buttons=\"['bold', 'italic', 'underline', 'anchor']\"\n          >{{ item.heading }}</h4>\n          <p\n            class=\"text-muted\"\n            data-field-string=\"items[{{loop.index0}}].text\"\n            data-toolbar-buttons=\"['bold', 'italic', 'underline', 'anchor']\"\n          >{{ item.text }}</p>\n        </div>\n      {% endfor %}\n    </div>\n  </div>\n</section>\n"}},"theme":{"navbar":{"bg_color":"#eaeaea","padding_top":10,"padding_bottom":10,"border_bottom_width":1,"border_bottom_color":"#d6d6d6"},"logo":{"text_color":"#005da0","text_hover_color":"#005da0","text_focus_color":"#005da0"},"navigation":{"text_color":"#555555","text_hover_color":"#555555","text_focus_color":"#555555","bg_text_color":"transparent","bg_text_hover_color":"transparent","bg_text_active_color":"#005da0","bg_text_focus_color":"#005da0"},"colors":{"body_bg_color":"transparent","heading_text_color":"#555555","text_color":"#555555","text_muted_color":"#555555","primary_bg_color":"#005da0","primary_text_color":"#005da0","selection_text_color":"#f1f1f1","selection_bg_color":"#005da0"},"typography":{"type_base_size":14}}}